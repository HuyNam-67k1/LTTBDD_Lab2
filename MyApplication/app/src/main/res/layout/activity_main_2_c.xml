<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_2_c">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/background" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="344dp"
        android:layout_height="627dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:srcCompat="@drawable/background_in"
        tools:layout_editor_absoluteY="7dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="134dp"
        android:layout_height="56dp"
        android:fontFamily="sans-serif-black"
        android:text="PASSWORD GENERATOR"
        android:textColor="#F3EFEF"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="48dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="302dp"
        android:layout_height="50dp"
        android:background="#100101"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Password length"
        android:textColor="#FBF7F7"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="231dp" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="127dp"
        android:layout_height="31dp"
        android:background="#FAF7F7"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="227dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="295dp"
        android:layout_height="61dp"
        android:fontFamily="sans-serif-black"
        android:text="GENERATE PASSWORD "
        android:textSize="20sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="531dp" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="306dp"
        android:layout_height="45dp"
        android:button="@null"
        android:checked="true"
        android:drawableRight="@drawable/rectangle_21"
        android:fontFamily="sans-serif-black"
        android:gravity="left"
        android:isScrollContainer="true"
        android:padding="5dp"
        android:shadowColor="#00F8F2F2"
        android:text="Include upcase letters"
        android:textColor="#FAF6F6"
        android:textColorHighlight="#FAF5F5"
        android:textColorLink="#FFFFFF"
        android:textSize="20sp"
        app:useMaterialThemeColors="true"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="339dp" />

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="306dp"
        android:layout_height="45dp"
        android:button="@null"
        android:checked="true"
        android:drawableRight="@drawable/rectangle_21"
        android:fontFamily="sans-serif-black"
        android:gravity="left"
        android:isScrollContainer="true"
        android:padding="5dp"
        android:shadowColor="#00F8F2F2"
        android:text="Include number"
        android:textColor="#FAF6F6"
        android:textColorHighlight="#FAF5F5"
        android:textColorLink="#FFFFFF"
        android:textSize="20sp"
        app:useMaterialThemeColors="true"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="402dp" />

    <CheckBox
        android:id="@+id/checkBox7"
        android:layout_width="306dp"
        android:layout_height="45dp"
        android:button="@null"
        android:checked="true"
        android:drawableRight="@drawable/rectangle_21"
        android:fontFamily="sans-serif-black"
        android:gravity="left"
        android:isScrollContainer="true"
        android:padding="5dp"
        android:shadowColor="#00F8F2F2"
        android:text="Include special symbol"
        android:textColor="#FAF6F6"
        android:textColorHighlight="#FAF5F5"
        android:textColorLink="#FFFFFF"
        android:textSize="20sp"
        app:useMaterialThemeColors="true"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="464dp" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="306dp"
        android:layout_height="45dp"
        android:button="@null"
        android:checked="true"
        android:drawableRight="@drawable/rectangle_21"
        android:fontFamily="sans-serif-black"
        android:gravity="left"
        android:isScrollContainer="true"
        android:padding="5dp"
        android:shadowColor="#00F8F2F2"
        android:text="Include lower case letters"
        android:textColor="#FAF6F6"
        android:textColorHighlight="#FAF5F5"
        android:textColorLink="#FFFFFF"
        android:textSize="20sp"
        app:useMaterialThemeColors="true"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="275dp" />

</androidx.constraintlayout.widget.ConstraintLayout>