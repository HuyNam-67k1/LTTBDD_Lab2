        tools:layout_editor_absoluteY="202dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="310dp"
        android:layout_height="72dp"
        android:fontFamily="sans-serif-medium"
        android:text="USB Bluetooth Music Receiver HJX-001-Biến loa thường thành loa bluetooth"
        android:textColor="#121111"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Cực kỳ hài lòng"
        android:textColor="#121111"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="141dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="333dp"
        android:layout_height="57dp"
        android:background="#2196F3"
        android:fontFamily="sans-serif-black"
        android:text="Gửi"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="646dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="332dp"
        android:layout_height="83dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:isScrollContainer="true"
        android:shadowColor="#835656"
        android:text="Thêm hình ảnh"
        android:textColor="#0B0A0A"
        android:textSize="16sp"
        app:backgroundTint="#FFFFFF"
        app:cornerRadius="6dp"
        app:icon="@drawable/camera"
        app:iconGravity="textStart"
        app:iconPadding="20dp"
        app:iconTint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#FAF9F9"
        app:strokeColor="#03A9F4"
        app:strokeWidth="2dp"
        tools:layout_editor_absoluteY="290dp" />

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="0dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="334dp"
        android:layout_height="229dp"
        android:drawablePadding="20dp"
        android:gravity="top|left"
        android:hint="Hãy chia sẻ những điều bạn thích về sản phẩm"
        android:shadowColor="#813737"
        android:textColor="#DAD5D5"
        android:textColorHint="#C8C3C3"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="394dp" />

</androidx.constraintlayout.widget.ConstraintLayout>